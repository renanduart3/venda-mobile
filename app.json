{
  "expo": {
    "name": "Loja Inteligente â€” Vendas & Estoque",
    "slug": "venda-mobile",
    "version": "1.0.0",
    "sdkVersion": "54.0.0",
    "icon": "./assets/images/icon.png",
    "scheme": "lojaapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "react-native-iap",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.1.10"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "permissions": [],
      "package": "com.renanduart3.vendamobile",
      "navigationBar": {
        "backgroundColor": "#3b3b4c",
        "barStyle": "light-content"
      },
      "statusBar": {
        "backgroundColor": "#3b3b4c",
        "barStyle": "light-content",
        "translucent": false
      }
    }
  }
}
